@article{rezzolla_using_2018,
	title = {Using gravitational-wave observations and quasi-universal relations to constrain the maximum mass of neutron stars},
	volume = {852},
	issn = {2041-8205, 2041-8213},
	url = {http://arxiv.org/abs/1711.00314},
	doi = {10.3847/2041-8213/aaa401},
	abstract = {Combining the {GW} observations of merging systems of binary neutron stars and quasi-universal relations, we set constraints on the maximum mass that can be attained by nonrotating stellar models of neutron stars. More specifically, exploiting the recent observation of the {GW} event {GW} 170817 and drawing from basic arguments on kilonova modeling of {GRB} 170817A, together with the quasi-universal relation between the maximum mass of nonrotating stellar models \$M\_\{{\textbackslash}rm {TOV}\}\$ and the maximum mass supported through uniform rotation \$M\_\{{\textbackslash}rm max\}={\textbackslash}left(1.20{\textasciicircum}\{+0.02\}\_\{-0.05\}{\textbackslash}right) M\_\{{\textbackslash}rm {TOV}\}\$ we set limits for the maximum mass to be \$ 2.01{\textasciicircum}\{+0.04\}\_\{-0.04\}{\textbackslash}leq M\_\{{\textbackslash}rm {TOV}\}/M\_\{{\textbackslash}odot\}{\textbackslash}lesssim 2.16{\textasciicircum}\{+0.17\}\_\{-0.15\}\$, where the lower limit in this range comes from pulsar observations. Our estimate, which follows a very simple line of arguments and does not rely on the modeling of the electromagnetic signal in terms of numerical simulations, can be further refined as new detections become available. We briefly discuss the impact that our conclusions have on the equation of state of nuclear matter.},
	pages = {L25},
	number = {2},
	journaltitle = {The Astrophysical Journal Letters},
	shortjournal = {Astrophys. J. Lett.},
	author = {Rezzolla, Luciano and Most, Elias R. and Weih, Lukas R.},
	urldate = {2024-01-18},
	date = {2018-01-09},
	eprinttype = {arxiv},
	eprint = {1711.00314 [astro-ph.HE]},
	note = {617 citations ({INSPIRE} 2024/1/18)
567 citations w/o self ({INSPIRE} 2024/1/18)},
	keywords = {Astrophysics - High Energy Astrophysical Phenomena, General Relativity and Quantum Cosmology},
}

