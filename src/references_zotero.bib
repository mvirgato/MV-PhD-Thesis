@article{agnes_search_2023,
	title = {Search for Dark-Matter–Nucleon Interactions via Migdal Effect with {DarkSide}-50},
	volume = {130},
	url = {https://arxiv.org/abs/2207.11967},
	doi = {10.1103/PhysRevLett.130.101001},
	abstract = {Dark matter elastic scattering off nuclei can result in the excitation and ionization of the recoiling atom through the so-called Migdal effect. The energy deposition from the ionization electron adds to the energy deposited by the recoiling nuclear system and allows for the detection of interactions of sub-{GeV}/c\${\textasciicircum}2\$ mass dark matter. We present new constraints for sub-{GeV}/c\${\textasciicircum}2\$ dark matter using the dual-phase liquid argon time projection chamber of the {DarkSide}-50 experiment with an exposure of (12306 \${\textbackslash}pm\$ 184) kg d. The analysis is based on the ionization signal alone and significantly enhances the sensitivity of {DarkSide}-50, enabling sensitivity to dark matter with masses down to 40 {MeV}/c\${\textasciicircum}2\$. Furthermore, it sets the most stringent upper limit on the spin independent dark matter nucleon cross section for masses below \$3.6\$ {GeV}/c\${\textasciicircum}2\$.},
	pages = {101001},
	number = {10},
	journaltitle = {Phys. Rev. Lett.},
	shortjournal = {Phys. Rev. Lett.},
	author = {Agnes, P. and {others}},
	date = {2023-03-06},
	eprinttype = {arxiv},
	eprint = {2207.11967 [hep-ex]},
	note = {42 citations ({INSPIRE} 2024/1/22)
30 citations w/o self ({INSPIRE} 2024/1/22)
tex.collaboration: {DarkSide}},
	keywords = {{WIMP} nucleon: elastic scattering, {WIMP} nucleus: elastic scattering, atom: excited state, dark matter: direct detection, dark matter: elastic scattering, dark matter: mass, dark matter: scattering, ionization, sensitivity, time projection chamber: liquid argon},
}

@article{meng_dark_2021,
	title = {Dark Matter Search Results from the {PandaX}-4T Commissioning Run},
	volume = {127},
	url = {https://arxiv.org/abs/2107.13438},
	doi = {10.1103/PhysRevLett.127.261802},
	abstract = {We report the first dark matter search results using the commissioning data from {PandaX}-4T. Using a time projection chamber with 3.7-tonne of liquid xenon target and an exposure of 0.63 tonne\${\textbackslash}cdot\$year, 1058 candidate events are identified within an approximate nuclear recoil energy window between 5 and 100 {keV}. No significant excess over background is observed. Our data set a stringent limit to the dark matter-nucleon spin-independent interactions, with a lowest excluded cross section (90\% C.L.) of \$3.8{\textbackslash}times10{\textasciicircum}\{-47\} \$cm\${\textasciicircum}2\$ at a dark matter mass of 30 {GeV}/\$c{\textasciicircum}2\$.},
	pages = {261802},
	number = {26},
	journaltitle = {Phys. Rev. Lett.},
	shortjournal = {Phys. Rev. Lett.},
	author = {Meng, Yue and {others}},
	date = {2021-12-23},
	eprinttype = {arxiv},
	eprint = {2107.13438 [hep-ex]},
	note = {359 citations ({INSPIRE} 2024/1/22)
317 citations w/o self ({INSPIRE} 2024/1/22)
tex.collaboration: {PandaX}-4T},
	keywords = {{WIMP} nucleon: interaction, {WIMP}: dark matter, background, dark matter: direct detection, dark matter: interaction, dark matter: mass, electron, time projection chamber, xenon: liquid, xenon: target},
}

