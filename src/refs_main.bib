@article{Bell:2011sn_Enhancedneutrinosignals,
  title        = {Enhanced Neutrino Signals from Dark Matter Annihilation in the {{Sun}} via Metastable Mediators},
  author       = {Bell, Nicole F. and Petraki, Kalliopi},
  date         = {2011},
  journaltitle = {Journal of Cosmology and Astroparticle Physics},
