@article{DarkSide:2022dhx_mar_SearchDarkMatterNucleon,
  title         = {Search for {{Dark-Matter}}\textendash{{Nucleon Interactions}} via {{Migdal Effect}} with {{DarkSide-50}}},
  author        = {Agnes, P. and others},
  date          = {2023-03-06},
  journaltitle  = {Phys. Rev. Lett.},
