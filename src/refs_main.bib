@article{Goodman:2010ku_ConstraintsDarkMatter,
  title        = {Constraints on {{Dark Matter}} from {{Colliders}}},
  author       = {Goodman, Jessica and Ibe, Masahiro and Rajaraman, Arvind and Shepherd, William and Tait, Tim M. P. and Yu, Hai-Bo},
  date         = {2010},
