@article{Bramante:2013nma_jan_Boundsselfinteractingfermiona,
  title        = {Bounds on Self-Interacting Fermion Dark Matter from Observations of Old Neutron Stars},
  author       = {Bramante, Joseph and Fukushima, Keita and Kumar, Jason and Stopnitzky, Elan},
