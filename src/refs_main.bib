@article{Anzuini:2021lnv_nov_Improvedtreatmentdark,
  title = {Improved Treatment of Dark Matter Capture in Neutron Stars {{III}}: Nucleon and Exotic Targets},
  shorttitle = {Improved Treatment of Dark Matter Capture in Neutron Stars {{III}}},
  author = {Anzuini, Filippo and Bell, Nicole F. and Busoni, Giorgio and Motta, Theo F. and Robles, Sandra and Thomas, Anthony W. and Virgato, Michael},
  date = {2021-11-29},
  journaltitle = {JCAP},
