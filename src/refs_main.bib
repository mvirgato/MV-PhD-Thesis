@article{Jungman:1995df_Supersymmetricdarkmatter,
  title        = {Supersymmetric Dark Matter},
  author       = {Jungman, Gerard and Kamionkowski, Marc and Griest, Kim},
  date         = {1996},
  journaltitle = {Phys. Rept.},
