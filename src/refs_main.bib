@article{Vincent:2013lua_apr_Thermalconductiondark,
  title        = {Thermal Conduction by Dark Matter with Velocity and Momentum-Dependent Cross-Sections},
  author       = {Vincent, Aaron C. and Scott, Pat},
  date         = {2014-04-22},
  journaltitle = {Journal of Cosmology and Astroparticle Physics},
  shortjournal = {JCAP},
  volume       = {04},
  number       = {04},
  eprint       = {1311.2074},
  eprinttype   = {arxiv},
  eprintclass  = {astro-ph.CO},
  pages        = {019},
  issn         = {1475-7516},
  doi          = {10.1088/1475-7516/2014/04/019},
  abstract     = {We use the formalism of Gould and Raffelt to compute the dimensionless thermal conduction coefficients for scattering of dark matter particles with standard model nucleons via cross-sections that depend on the relative velocity or momentum exchanged between particles. Motivated by models invoked to reconcile various recent results in direct detection, we explicitly compute the conduction coefficients \$\textbackslash alpha\$ and \$\textbackslash kappa\$ for cross-sections that go as \$v\_\{\textbackslash rm rel\}\^2\$, \$v\_\{\textbackslash rm rel\}\^4\$, \$v\_\{\textbackslash rm rel\}\^\{-2\}\$, \$q\^2\$, \$q\^4\$ and \$q\^\{-2\}\$, where \$v\_\{\textbackslash rm rel\}\$ is the relative DM-nucleus velocity and \$q\$ is the momentum transferred in the collision. We find that a \$v\_\{\textbackslash rm rel\}\^\{-2\}\$ dependence can significantly enhance energy transport from the inner solar core to the outer core. The same can true for any \$q\$-dependent coupling, if the dark matter mass lies within some specific range for each coupling. This effect can complement direct searches for dark matter; combining these results with state-of-the-art Solar simulations should greatly increase sensitivity to certain DM models. It also seems possible that the so-called Solar Abundance Problem could be resolved by enhanced energy transport in the solar core due to such velocity- or momentum-dependent scatterings.},
  keywords     = {Astrophysics - Cosmology and Nongalactic Astrophysics,Astrophysics - Solar and Stellar Astrophysics,High Energy Physics - Phenomenology},
  annotation   = {35 citations (INSPIRE 2024/2/12) 26 citations w/o self (INSPIRE 2024/2/12)},
  file         = {/home/student.unimelb.edu.au/mvirgato/Zotero/storage/9HMCVACM/Vincent and Scott - 2014 - Thermal conduction by dark matter with velocity an.pdf;/home/student.unimelb.edu.au/mvirgato/Zotero/storage/EAX7PP76/1311.html}
}

