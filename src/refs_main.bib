@article{McCullough:2010ai_CaptureInelasticDark,
  title        = {Capture of {{Inelastic Dark Matter}} in {{White Dwarves}}},
  author       = {McCullough, Matthew and Fairbairn, Malcolm},
  date         = {2010},
  journaltitle = {Physical Review D},
  shortjournal = {Phys. Rev. D},
  volume       = {81},
  number       = {8},
  eprint       = {1001.2737},
  eprinttype   = {arxiv},
  eprintclass  = {hep-ph},
  pages        = {083520},
  issn         = {1550-7998, 1550-2368},
  doi          = {10.1103/PhysRevD.81.083520},
  abstract     = {We consider the capture of inelastic dark matter in white dwarves by inelastic spin-independent scattering on nuclei. We show that if the dark matter annihilates to standard-model particles then, under the assumption of primordial globular cluster formation, the observation of cold white dwarves in the globular cluster M4 appears inconsistent with explanations of the observed DAMA/LIBRA annual modulation signal based on spin-independent inelastic dark matter scattering. Alternatively if the inelastic dark matter scenario were to be confirmed and it was found to annihilate to standard-model particles then this would imply a much lower dark matter density in the core of M4 than would be expected if it were to have formed in a dark matter halo. Finally we argue that cold white dwarves constitute a unique dark matter probe, complementary to other direct and indirect detection searches.},
  keywords     = {Astrophysics - Cosmology and Nongalactic Astrophysics,High Energy Physics - Phenomenology},
  annotation   = {112 citations (INSPIRE 2024/2/12) 107 citations w/o self (INSPIRE 2024/2/12)},
  file         = {/home/student.unimelb.edu.au/mvirgato/Zotero/storage/YJ4284CH/McCullough and Fairbairn - 2010 - Capture of Inelastic Dark Matter in White Dwarves.pdf;/home/student.unimelb.edu.au/mvirgato/Zotero/storage/GZIWQUZH/1001.html}
}

@article{McDermott:2011jp_ConstraintsScalarAsymmetric,
  title        = {Constraints on {{Scalar Asymmetric Dark Matter}} from {{Black Hole Formation}} in {{Neutron Stars}}},
  author       = {McDermott, Samuel D. and Yu, Hai-Bo and Zurek, Kathryn M.},
  date         = {2012},
  journaltitle = {Phys. Rev. D},
  shortjournal = {Phys. Rev. D},
  volume       = {85},
  eprint       = {1103.5472},
  eprinttype   = {arxiv},
  eprintclass  = {hep-ph},
  pages        = {023519},
  doi          = {10.1103/PhysRevD.85.023519},
  abstract     = {We consider possibly observable effects of asymmetric dark matter (ADM) in neutron stars. Since dark matter does not self-annihilate in the ADM scenario, dark matter accumulates in neutron stars, eventually reaching the Chandrasekhar limit and forming a black hole. We focus on the case of scalar ADM, where the constraints from Bose-Einstein condensation and subsequent black hole formation are most severe due to the absence of Fermi degeneracy pressure. We also note that in some portions of this constrained parameter space, non-trivial effects from Hawking radiation can modify our limits. We find that for scalar ADM with mass between 100 keV and 10\^5 GeV, the constraint from pulsars in globular clusters on the scattering cross-section with neutrons ranges from \textbackslash sigma\_n {$<$} 10\^\{-45\} cm\^2 to 10\^\{-52\} cm\vphantom\{\}\^2. In particular, for scalar ADM with mass between 1 GeV and 1 TeV (in the case where black hole evaporation due to Hawking radiation is unimportant), the constraint on the scattering cross-section is below what is reachable with ton scale direct detection experiments.},
  keywords     = {95.35.+d,98.80.Cq,Astrophysics - Cosmology and Nongalactic Astrophysics,black hole: formation,condensation: Bose-Einstein,dark matter: capture,dark matter: density,dark matter: mass,dark matter: scalar,High Energy Physics - Phenomenology,neutron star,pulsar},
  annotation   = {210 citations (INSPIRE 2024/1/22) 197 citations w/o self (INSPIRE 2024/1/22)},
  file         = {/home/student.unimelb.edu.au/mvirgato/Zotero/storage/NHTB4TKU/McDermott et al. - 2012 - Constraints on Scalar Asymmetric Dark Matter from .pdf;/home/student.unimelb.edu.au/mvirgato/Zotero/storage/B8BULFXJ/1103.html}
}

