@article{CRESST:2019jnq_nov_FirstresultsCRESSTIII,
  title         = {First Results from the {{CRESST-III}} Low-Mass Dark Matter Program},
  author        = {Abdelhameed, A.H. and Angloher, G. and Bauer, P. and others},
