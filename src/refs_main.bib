@article{Bramante:2017ulk_mar_SearchingDarkMatter,
  title        = {Searching for {{Dark Matter}} with {{Neutron Star Mergers}} and {{Quiet Kilonovae}}},
  author       = {Bramante, Joseph and Linden, Tim and Tsai, Yu-Dai},
  date         = {2018-03-12},
  journaltitle = {Physical Review D},
  shortjournal = {Phys. Rev. D},
  volume       = {97},
  number       = {5},
  eprint       = {1706.00001},
  eprinttype   = {arxiv},
  eprintclass  = {hep-ph},
  pages        = {055016},
  issn         = {2470-0010, 2470-0029},
  doi          = {10.1103/PhysRevD.97.055016},
  abstract     = {We identify new astrophysical signatures of dark matter that implodes neutron stars (NSs), which could decisively test whether NS-imploding dark matter is responsible for missing pulsars in the Milky Way galactic center, the source of some \$r\$-process elements, and the origin of fast-radio bursts. First, NS-imploding dark matter forms \$\textbackslash sim 10\^\{-10\}\$ solar mass or smaller black holes inside neutron stars, which proceed to convert neutron stars into \$\textbackslash sim\$1.5 solar mass BHs. This decreases the number of neutron star mergers seen by LIGO/Virgo (LV) and associated merger kilonovae seen by telescopes like DES, BlackGEM, and ZTF, and instead, producing a population of "black mergers" containing \$\textbackslash sim\$1.5 solar mass black holes. Second, dark matter-induced neutron star implosions may create a new kind of kilonovae that lacks a detectable, accompanying gravitational signal, which we call "quiet kilonovae." Using DES data and the Milky Way's r-process abundance, we constrain quiet kilonovae. Third, the spatial distribution of neutron star merger kilonovae and quiet kilonovae in galaxies can be used to detect dark matter. NS-imploding dark matter destroys most neutron stars at the centers of disc galaxies, so that neutron star merger kilonovae would appear mostly in a donut at large radii. We find that as few as ten neutron star merger kilonova events, located to \$\textbackslash sim\$1 kpc precision could validate or exclude dark matter-induced neutron star implosions at \$2 \textbackslash sigma\$ confidence, exploring dark matter-nucleon cross-sections 4-10 orders of magnitude below current direct detection experimental limits. Similarly, NS-imploding dark matter as the source of fast radio bursts can be tested at \$2 \textbackslash sigma\$ confidence once 20 bursts are located in host galaxies by radio arrays like CHIME and HIRAX.},
  keywords     = {Astrophysics - High Energy Astrophysical Phenomena,High Energy Physics - Phenomenology},
  annotation   = {66 citations (INSPIRE 2024/2/12) 55 citations w/o self (INSPIRE 2024/2/12)},
  file         = {/home/student.unimelb.edu.au/mvirgato/Zotero/storage/NJ6E82ZZ/Bramante et al. - 2018 - Searching for Dark Matter with Neutron Star Merger.pdf;/home/student.unimelb.edu.au/mvirgato/Zotero/storage/UH8WITW3/1706.html}
}

