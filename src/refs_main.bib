@article{Chen:2018lsk_apr_Evolutionprocesstwocomponent,
  title        = {On the Evolution Process of Two-Component Dark Matter in the {{Sun}}},
  author       = {Chen, Chian-Shu and Lin, Yen-Hsun},
  date         = {2018-04-13},
