@article{Tews:2018kmu_jun_Constrainingspeedsound,
  title        = {Constraining the Speed of Sound inside Neutron Stars with Chiral Effective Field Theory Interactions and Observations},
  author       = {Tews, Ingo and Carlson, Joseph and Gandolfi, Stefano and Reddy, Sanjay},
  date         = {2018-06-21},
  journaltitle = {The Astrophysical Journal},
  shortjournal = {Astrophys. J.},
  volume       = {860},
  number       = {2},
  eprint       = {1801.01923},
  eprinttype   = {arxiv},
  eprintclass  = {nucl-th},
  pages        = {149},
  issn         = {0004-637X, 1538-4357},
  doi          = {10.3847/1538-4357/aac267},
  abstract     = {The dense matter equation of state (EOS) determines neutron star (NS) structure but can be calculated reliably only up to one to two times the nuclear saturation density, using accurate many-body methods that employ nuclear interactions from chiral effective field theory constrained by scattering data. In this work, we use physically motivated ansatzes for the speed of sound \$c\_S\$ at high density to extend microscopic calculations of neutron-rich matter to the highest densities encountered in stable NS cores. We show how existing and expected astrophysical constraints on NS masses and radii from X-ray observations can constrain the speed of sound in the NS core. We confirm earlier expectations that \$c\_S\$ is likely to violate the conformal limit of \$c\_S\^{}2\textbackslash leq c\^{}2/3 \$, possibly reaching values closer to the speed of light \$c\$ at a few times the nuclear saturation density, independent of the nuclear Hamiltonian. If QCD obeys the conformal limit, we conclude that the rapid increase of \$c\_S\$ required to accommodate a \$2 \$ M\$\_\textbackslash odot\$ NS suggests a form of strongly interacting matter where a description in terms of nucleons will be unwieldy, even between one and two times the nuclear saturation density. For typical NSs with masses in the range \$1.2-1.4\textasciitilde\$ M\$\_\textbackslash odot\$, we find radii between \$10\$ and \$14\$ km, and the smallest possible radius of a \$1.4\$ M\$\_\{\textbackslash odot\}\$ NS consistent with constraints from nuclear physics and observations is \$8.4\$ km. We also discuss how future observations could constrain the EOS and guide theoretical developments in nuclear physics.},
  keywords     = {High Energy Physics - Phenomenology,Nuclear Theory},
  annotation   = {292 citations (INSPIRE 2024/2/28) 243 citations w/o self (INSPIRE 2024/2/28)},
  file         = {/home/student.unimelb.edu.au/mvirgato/Zotero/storage/XGXSS5FG/Tews et al. - 2018 - Constraining the speed of sound inside neutron sta.pdf;/home/student.unimelb.edu.au/mvirgato/Zotero/storage/XZHLD4F3/1801.html}
}

