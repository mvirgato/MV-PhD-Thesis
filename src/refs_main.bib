}

@article{NANOGrav:2019jur_sep_RelativisticShapirodelay,
  title         = {Relativistic {{Shapiro}} Delay Measurements of an Extremely Massive Millisecond Pulsar},
  author        = {Cromartie, H. T. and Fonseca, E. and Ransom, S. M. and Demorest, P. B. and Arzoumanian, Z. and Blumer, H. and Brook, P. R. and DeCesar, M. E. and Dolch, T. and Ellis, J. A. and Ferdman, R. D. and Ferrara, E. C. and Garver-Daniels, N. and Gentile, P. A. and Jones, M. L. and Lam, M. T. and Lorimer, D. R. and Lynch, R. S. and McLaughlin, M. A. and Ng, C. and Nice, D. J. and Pennucci, T. T. and Spiewak, R. and Stairs, I. H. and Stovall, K. and Swiggum, J. K. and Zhu, W. W.},
  date          = {2019-09-16},
  journaltitle  = {Nature Astronomy},
  shortjournal  = {Nature Astron.},
  volume        = {4},
  number        = {1},
  eprint        = {1904.06759},
  eprinttype    = {arxiv},
  eprintclass   = {astro-ph.HE},
  pages         = {72--76},
  issn          = {2397-3366},
  doi           = {10.1038/s41550-019-0880-2},
  abstract      = {Despite its importance to our understanding of physics at supranuclear densities, the equation of state (EoS) of matter deep within neutron stars remains poorly understood. Millisecond pulsars (MSPs) are among the most useful astrophysical objects in the Universe for testing fundamental physics, and place some of the most stringent constraints on this high-density EoS. Pulsar timing---the process of accounting for every rotation of a pulsar over long time periods---can precisely measure a wide variety of physical phenomena, including those that allow the measurement of the masses of the components of a pulsar binary system1. One of these, called relativistic Shapiro delay2, can yield precise masses for both an MSP and its companion; however, it is only easily observed in a small subset of high-precision, highly inclined (nearly edge-on) binary pulsar systems. By combining data from the North American Nanohertz Observatory for Gravitational Waves (NANOGrav) 12.5-yr data set with recent orbital-phase-specific observations using the Green Bank Telescope, we have measured the mass of the MSP J0740+6620 to be 2 .14-0.09 +0 .10 ? M{$\odot$} (68.3\% credibility interval; the 95.4\% credibility interval is 2 .14-0.18 +0 .20 ? M{$\odot$}). It is highly likely to be the most massive neutron star yet observed, and serves as a strong constraint on the neutron star interior EoS.},
  collaboration = {NANOGrav},
  keywords      = {Astrophysics - High Energy Astrophysical Phenomena},
  annotation    = {1235 citations (INSPIRE 2024/2/28) 1204 citations w/o self (INSPIRE 2024/2/28) ADS Bibcode: 2020NatAs...4...72C},
  file          = {/home/student.unimelb.edu.au/mvirgato/Zotero/storage/T5MJ3VFX/Cromartie et al. - 2020 - Relativistic Shapiro delay measurements of an extr.pdf}
}
@article{De:2018uhw_aug_TidalDeformabilitiesRadii,
  title        = {Tidal {{Deformabilities}} and {{Radii}} of {{Neutron Stars}} from the {{Observation}} of {{GW170817}}},
  author       = {De, Soumi and Finstad, Daniel and Lattimer, James M. and Brown, Duncan A. and Berger, Edo and Biwer, Christopher M.},
  date         = {2018-08-29},
  journaltitle = {Physical Review Letters},
  shortjournal = {Phys. Rev. Lett.},
  volume       = {121},
  number       = {9},
  eprint       = {1804.08583},
  eprinttype   = {arxiv},
  eprintclass  = {astro-ph.HE},
  pages        = {091102},
  issn         = {0031-9007, 1079-7114},
  doi          = {10.1103/PhysRevLett.121.091102},
  abstract     = {We use gravitational-wave observations of the binary neutron star merger GW170817 to explore the tidal deformabilities and radii of neutron stars. We perform Bayesian parameter estimation with the source location and distance informed by electromagnetic observations. We also assume that the two stars have the same equation of state; we demonstrate that for stars with masses comparable to the component masses of GW170817, this is effectively implemented by assuming that the stars' dimensionless tidal deformabilities are determined by the binary's mass ratio \$q\$ by \$\textbackslash Lambda\_1/\textbackslash Lambda\_2 = q\^{}6\$. We investigate different choices of prior on the component masses of the neutron stars. We find that the tidal deformability and 90\$\textbackslash\%\$ credible interval is \$\textbackslash tilde\{\textbackslash Lambda\}=222\^{}\{+420\}\_\{-138\}\$ for a uniform component mass prior, \$\textbackslash tilde\{\textbackslash Lambda\}=245\^{}\{+453\}\_\{-151\}\$ for a component mass prior informed by radio observations of Galactic double neutron stars, and \$\textbackslash tilde\{\textbackslash Lambda\}=233\^{}\{+448\}\_\{-144\}\$ for a component mass prior informed by radio pulsars. We find a robust measurement of the common areal radius of the neutron stars across all mass priors of \$8.9 \textbackslash le \textbackslash hat\{R\} \textbackslash le 13.2\$ km, with a mean value of \$\textbackslash langle \textbackslash hat\{R\} \textbackslash rangle = 10.8\$ km. Our results are the first measurement of tidal deformability with a physical constraint on the star's equation of state and place the first lower bounds on the deformability and areal radii of neutron stars using gravitational waves.},
  keywords     = {Astrophysics - High Energy Astrophysical Phenomena,General Relativity and Quantum Cosmology},
  annotation   = {573 citations (INSPIRE 2024/2/29) 539 citations w/o self (INSPIRE 2024/2/29)},
  file         = {/home/student.unimelb.edu.au/mvirgato/Zotero/storage/AI36T3XT/De et al. - 2018 - Tidal Deformabilities and Radii of Neutron Stars f.pdf;/home/student.unimelb.edu.au/mvirgato/Zotero/storage/L93M5LWC/1804.html}
}

