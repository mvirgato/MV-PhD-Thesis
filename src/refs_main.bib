@article{Ellis:2018bkr_jun_DarkMatterEffects,
  title        = {Dark {{Matter Effects On Neutron Star Properties}}},
  author       = {Ellis, John and H\"utsi, Gert and Kannike, Kristjan and Marzola, Luca and Raidal, Martti and Vaskonen, Ville},
  date         = {2018-06-15},
