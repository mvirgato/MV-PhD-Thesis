@article{PICO:2019vsc_jul_Darkmattersearch,
  title         = {Dark Matter Search Results from the Complete Exposure of the {{PICO-60}} \$\{\textbackslash mathrm\{\vphantom{\}\}}{{C}}\vphantom\{\}\vphantom\{\}\_\{3\}\{\textbackslash mathrm\{\vphantom{\}\}}{{F}}\vphantom\{\}\vphantom\{\}\_\{8\}\$ Bubble Chamber},
