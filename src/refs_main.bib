@article{Bell:2020jou_sep_ImprovedTreatmentDark,
  title        = {Improved {{Treatment}} of {{Dark Matter Capture}} in {{Neutron Stars}}},
  author       = {Bell, Nicole F. and Busoni, Giorgio and Robles, Sandra and Virgato, Michael},
