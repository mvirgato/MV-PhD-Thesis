}

@article{McDermott:2010pa_TurningLightsHow,
  title        = {Turning off the {{Lights}}: {{How Dark}} Is {{Dark Matter}}?},
  shorttitle   = {Turning off the {{Lights}}},
  author       = {McDermott, Samuel D. and Yu, Hai-Bo and Zurek, Kathryn M.},
  date         = {2011},
  journaltitle = {Physical Review D},
  shortjournal = {Phys. Rev. D},
  volume       = {83},
  number       = {6},
  eprint       = {1011.2907},
  eprinttype   = {arxiv},
